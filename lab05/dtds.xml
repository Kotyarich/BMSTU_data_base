        <!ELEMENT flights (row)*>
        <!ATTLIST flights
                xmlns:xsi CDATA #REQUIRED>
        <!ELEMENT row (id|plane_id|departure_airport_id|arrival_airport_id|departure_time|arrival_time)*>
        <!ELEMENT id (#PCDATA)>
        <!ELEMENT plane_id (#PCDATA)>
        <!ELEMENT departure_airport_id (#PCDATA)>
        <!ELEMENT arrival_airport_id (#PCDATA)>
        <!ELEMENT departure_time (#PCDATA)>
        <!ELEMENT arrival_time (#PCDATA)>
        ]>

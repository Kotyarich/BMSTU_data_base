<!DOCTYPE airports [
        <!ELEMENT airports (row)*>
        <!ATTLIST airports
                xmlns:xsi CDATA #REQUIRED>
        <!ELEMENT row (id|name|country|city)*>
        <!ELEMENT id (#PCDATA)>
        <!ELEMENT name (#PCDATA)>
        <!ELEMENT country (#PCDATA)>
        <!ELEMENT city (#PCDATA)>
        ]>
